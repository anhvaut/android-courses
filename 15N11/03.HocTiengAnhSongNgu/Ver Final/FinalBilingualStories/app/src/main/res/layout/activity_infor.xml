<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="btob.finalbilingualstories.InforActivity"
    android:background="@drawable/pic3"
    android:orientation="vertical"
    android:gravity="center">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Thông tin Nhóm"
        android:textColor="@color/white"
        android:gravity="center"
        android:textSize="30dp"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="50dp" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dương Hồng Hà"
        android:textColor="@color/white"
        android:gravity="center"
        android:textSize="20dp"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20dp" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nguyễn Văn Quang Huy"
        android:textColor="@color/white"
        android:gravity="center"
        android:textSize="20dp"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20dp" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tạ Nhật Quang"
        android:textColor="@color/white"
        android:gravity="center"
        android:textSize="20dp"
        />
    <Space
        android:layout_width="match_parent"
        android:layout_height="20dp" />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Trương Thế Quốc Sỹ"
        android:textColor="@color/white"
        android:gravity="center"
        android:textSize="20dp"
        />

</LinearLayout>
